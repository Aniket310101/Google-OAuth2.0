{
  "name": "google-drive-api",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon --watch src -e .ts,.json,.tsx --exec \"tsc && node dist/main.js\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "1.6.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "googleapis": "105.0.0",
    "prettier": "^3.2.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "8.0.0",
    "lint-staged": "15.2.0",
    "nodemon": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx, js, jsx}": [
      "git add"
    ],
    "src/**/*.{ts, tsx, js, jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
